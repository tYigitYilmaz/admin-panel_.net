
<link href="/plugins/bower_components/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css" />


<!-- .row -->
@using ( Html.BeginForm( "AddCashEnd", "CashBook", FormMethod.Post, new { @class = "form-horizontal", data_toggle = "validator" } ) ) {
    <div class="panel panel-default" style="margin-top: 0">
        <div class="panel-heading">
            Kasa Devir <small class="text-muted"> Gün Sonu </small>
        </div>
        <div class="panel-wrapper">
            <div class="panel-body">
                <input id="Id" name="Id" type="hidden" value="@ViewBag.CashBookId" />

                @Html.Partial( "Partial/_CashEndForm" )

                <div class="form-group m-b-0">
                    <div class="pull-right">
                        <button type="button" class="btn btn-default waves-effect waves-light m-t-10" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Back</span></button>
                        <button type="submit" class="btn btn-info waves-effect waves-light m-t-10">Save</button>
                    </div>
                </div>

            </div>
        </div>
    </div>

}

<!-- /.container-fluid -->

<script src="/plugins/bower_components/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
<script>
    jQuery(document).ready(function ($) {
        jQuery('.mydatepicker, #datepicker').datepicker({
            format: 'dd/mm/yyyy'
        });


    });
</script>




